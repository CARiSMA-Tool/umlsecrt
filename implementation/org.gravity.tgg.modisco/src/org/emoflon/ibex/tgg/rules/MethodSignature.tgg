#using org.gravity.tgg.modisco.*
#using AttrCondDefLibrary.*

#rule MethodSignature
#with org.gravity.tgg.modisco

#source {
	++ mSignature : MMethodSignature {
		++ -mMethodName-> mName
	}
	mName : MMethodName {
		++  -mSignatures -> mSignature
	}
}

#target {
	++ tSignature : TMethodSignature {
		++  -method -> tName
	}
	tName : TMethod 
}

#correspondence {
	++ mSignatureToTSignature : MSignatureToTSignature {
		#src->mSignature
		#trg->tSignature
	}
	
	++mFlowElementToTFlowElement:MAbstractFlowElementToTAbstractFlowElement {
		#src->mSignature
		#trg->tSignature
	}
	
	++ mMethodSignatureToTMethodSignature : MMethodSignatureToTMethodSignature {
		#src->mSignature
		#trg->tSignature
	}
	++mParameterListToTMethodSignature : MParameterListToTMethodSignature {
		#src->mSignature
		#trg->tSignature
	}
	mNameToTName : MMethodNameToTMethod {
		#src->mName
		#trg->tName
	}
}