#using org.gravity.tgg.modisco.*
#using AttrCondDefLibrary.*

#rule AbstractMethodDefinitionBloc
#with org.gravity.tgg.modisco

#source {
	mDefinition:MAbstractMethodDefinition {
		++ -body -> mBlock
	}
	
	++ mBlock:Block
}

#target {
	tDefinition:TMethodDefinition
}

#correspondence {
	mDefinitionToTDefinition :MDefinitionToTMember {
		#src->mDefinition
		#trg->tDefinition
	}

	++ mBlockToTDefinition :BlockToTMethodDefinition {
		#src->mBlock
		#trg->tDefinition
	}
}