#using org.gravity.tgg.modisco.pm.*
#using AttrCondDefLibrary.*

#rule Class
#extends Types
#with org.gravity.tgg.modisco.pm

#source {
	++mType:MClass
}

#target {
	++tType:TClass

	tPackage:TPackage {
		++  -classes -> tType
	}

	pg:TypeGraph {
		++  -classes -> tType
	}
}

#correspondence {
	++ mClassDeclarationToTClass :ClassDeclarationToTClass {
		#src->mType
		#trg->tType
	}
}

#attributeConditions {
	eq(mType.name, tType.tName)
}