#using org.gravity.tgg.modisco.*
#using AttrCondDefLibrary.*

#rule NestedEnum
#extends NestedTypes
#with org.gravity.tgg.modisco

#source {
	++eNestedType:EnumDeclaration
}

#target {
	typeGraph:TypeGraph {
		++  -ownedTypes -> tNestedType
		++  -classes -> tNestedType
	}

	++tNestedType:TClass {
		++-tAnnotation -> tAnnotation
	}

	++tAnnotation:TEnum

	++tNestedPackage:TPackage {
		++  -ownedTypes -> tNestedType
		++  -classes -> tNestedType
	}
}

#correspondence {
	++ eNestedTypeToTNestedType :TypeToTAbstractType {
		#src->eNestedType
		#trg->tNestedType
	}
}

#attributeConditions {
	eq(eNestedType.name, tNestedType.tName)
}
