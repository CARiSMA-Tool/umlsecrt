#using org.gravity.tgg.modisco.*
#using AttrCondDefLibrary.*

#rule Interface
#extends Types
#with org.gravity.tgg.modisco

#source {
	++ mType : InterfaceDeclaration {
	}
}

#target {
	++ tType : TInterface {
	}
	pg : TypeGraph {
		++  -interfaces -> tType
	}
	tPackage : TPackage {
		++  -interfaces -> tType
	}
}

#correspondence {
	++ mInterfaceDeclarationtoTInterface : InterfaceDeclarationToTInterface {
		#src->mType
		#trg->tType
	}
}

#attributeConditions {
	eq(mType.name, tType.tName)
}