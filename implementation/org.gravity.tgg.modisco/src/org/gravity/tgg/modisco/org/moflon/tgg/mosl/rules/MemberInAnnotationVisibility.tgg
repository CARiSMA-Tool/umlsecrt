#using org.gravity.tgg.modisco.*
#using AttrCondDefLibrary.*

#rule MemberInAnnotationVisibility #with org.gravity.tgg.modisco

#source { 
		mAnnotation : AnnotationTypeDeclaration {
		-bodyDeclarations->mDefinition
	}
	
	mDefinition:MDefinition{
		++ -modifier->mModifier
	}
	
	++mModifier:Modifier{	
	}
}

#target {
	tAnnotation : TAnnotationType {
		-defines->tMember
	}

	tMember:TMember{
		++ -tModifier->tModifier
	}	
	++ tModifier:TModifier{
		
	}
}

#correspondence {
mTypeToTType : TypeToTAbstractType {
	#src->mAnnotation
	#trg->tAnnotation
}

defToMember:MDefinitionToTMember{
	#src->mDefinition
	#trg->tMember
}

++ mModifierToTModifier:MModifierToTModifier{
	#src->mModifier
	#trg->tModifier
}
}

#attributeConditions {
	DefaultVisibility(mModifier.visibility, tModifier.tVisibility)
	eq(mModifier.static, tModifier.isStatic)
}