#using org.gravity.tgg.modisco.pm.*
#using AttrCondDefLibrary.*

#rule TypesOrphaned
#with org.gravity.tgg.modisco.pm

#source {
	mModel:Model {
		++  -orphanTypes -> mType
	}

	++mType: MClass
}

#target {
	pg:TypeGraph {
		++  -ownedTypes -> tType
	}

	++tType:TClass
}

#correspondence {
	mModelToTypeGraph :ModelToTypeGraph {
		#src->mModel
		#trg->pg
	}

	++mTypeToTType :TypeToTAbstractType {
		#src->mType
		#trg->tType
	}
}

#attributeConditions {
	eq(mType.name, tType.tName)
}