#using org.gravity.tgg.modisco.pm.*
#using AttrCondDefLibrary.*

#rule NestedClassInType
#extends NestedTypes
#with org.gravity.tgg.modisco.pm

#source {
	++eNestedType:MClass
}

#target {
	++tNestedType:TClass 

	tOuterPackage:TPackage {
		++  -classes -> tNestedType
	}

	typeGraph:TypeGraph {
		++  -classes -> tNestedType
	}
}

#attributeConditions {
	eq(eNestedType.name, tNestedType.tName)
}


