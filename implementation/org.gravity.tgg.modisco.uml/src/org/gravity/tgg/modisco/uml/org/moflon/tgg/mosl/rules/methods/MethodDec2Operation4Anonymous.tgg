#using org.gravity.tgg.modisco.uml.*
#using AttrCondDefLibrary.*

#rule MethodDec2Operation4Anonymous #extends MethodDec2Operation_Abstract #with org.gravity.tgg.modisco.uml

#source {
	classDec:AnonymousClassDeclaration {
		++ -bodyDeclarations -> bodyDeclaration
	}

	++bodyDeclaration:AbstractMethodDeclaration
}

#target {
	umlClass:Class {
		++ -ownedOperation -> feature
	}

	++feature:Operation
}


