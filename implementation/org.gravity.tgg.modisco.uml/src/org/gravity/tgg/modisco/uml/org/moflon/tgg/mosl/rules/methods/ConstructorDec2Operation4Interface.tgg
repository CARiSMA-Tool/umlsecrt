#using org.gravity.tgg.modisco.uml.*
#using AttrCondDefLibrary.*

#rule ConstructorDec2Operation4Interface #extends ConstructorDec2Operation_Abstract #with org.gravity.tgg.modisco.uml

#source {
	classDec:InterfaceDeclaration {
		++ -bodyDeclarations -> bodyDeclaration
	}

	++bodyDeclaration:MConstructorDefinition
}

#target {
	umlClass:Interface {
		++ -ownedOperation -> feature
	}

	++feature:Operation
}