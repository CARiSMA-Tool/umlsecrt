grant codeBase "file:${user.dir}/bin/-" {
	permission java.lang.RuntimePermission "printStackTrace()";
	permission java.lang.RuntimePermission "printStackTrace(PrintStream)";
	permission java.lang.RuntimePermission "printStackTrace(PrintWriter)";
	permission java.lang.RuntimePermission "closeClassLoader";
	permission java.io.FilePermission "<<ALL FILES>>", "read";
	permission java.util.PropertyPermission "user.dir", "read";
};
grant codeBase "file:${user.dir}/good/-" {
	permission java.lang.RuntimePermission "printStackTrace()";
	permission java.lang.RuntimePermission "printStackTrace(PrintStream)";
	permission java.lang.RuntimePermission "printStackTrace(PrintWriter)";
};